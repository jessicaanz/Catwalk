<mujoco model="cat">
  <compiler angle="radian" meshdir="meshes"/>
  
  <option integrator="implicitfast" cone="elliptic" timestep="0.002" gravity="0 0 -9.81"/>
  
  <visual>
    <global ellipsoidinertia="true"/>
  </visual>

  <asset>
    <material name="gray" rgba="0.3 0.3 0.3 1"/>

    <mesh name="base_link" file="base_link.STL"/>
    <mesh name="upperleg_fl" file="upperleg_fl.STL"/>
    <mesh name="lowerleg_fl" file="lowerleg_fl.STL"/>
    <mesh name="upperleg_fr" file="upperleg_fr.STL"/>
    <mesh name="lowerleg_fr" file="lowerleg_fr.STL"/>
    <mesh name="upperleg_bl" file="upperleg_bl.STL"/>
    <mesh name="lowerleg_bl" file="lowerleg_bl.STL"/>
    <mesh name="upperleg_br" file="upperleg_br.STL"/>
    <mesh name="lowerleg_br" file="lowerleg_br.STL"/>
  </asset>

  <default>
    <default class="cat">
      <geom solref="0.004 1"/>
      <joint actuatorfrcrange="-1000 1000"/>
      <position kp="10" kv="50" inheritrange="1"/>
      <default class="visual">
        <geom group="2" type="mesh" contype="1" conaffinity="1"/>
      </default>
    </default>
  </default>

<worldbody>
    <light name="spotlight" mode="targetbodycom" target="body" pos="3 0 4" cutoff="30"/>

    <!-- BODY -->
    <body name="body" childclass="cat" pos="0 0 -0.6">
      <light name="tracking" mode="trackcom" pos="0 0 2"/>
      <freejoint name="freejoint"/>
      <geom mesh="base_link" material="gray" class="visual"/>

      <!-- FL LEG -->
      <body name="upperleg_fl" pos="0.72627 -1.3017 0.72616" quat="0.377866 2.0129e-06 0.92586 2.0129e-06">
        <inertial pos="0.024758 -0.02325 0.024007" quat="0.275621 0.27562 -0.651178 0.651178" mass="0.060463" diaginertia="1.94562e-05 1.45858e-05 1.3106e-05"/>
        <joint name="hip_fl" pos="0 0 0" axis="0 1 0" range="-1.57 1.57"/>
        <geom material="gray" mesh="upperleg_fl" class="visual"/>
        <body name="lowerleg_fl" pos="0.039486 0 0.038287" quat="-3.40178e-06 0.377264 1.38577e-06 0.926106">
          <inertial pos="-4.75474e-09 0.02325 0.0364632" quat="1 -7.49515e-07 3.35811e-12 -4.48038e-06" mass="0.0699581" diaginertia="3.15837e-05 2.19759e-05 1.91147e-05"/>
          <joint name="knee_fl" pos="0 0 0" axis="0 1 0" range="-1.57 1.57"/>
          <geom material="gray" mesh="lowerleg_fl" class="visual" friction="5 0.1 0.1"/>
          <!-- Collision sphere -->
          <geom name="foot_fl" type="sphere" size="0.02" pos="0 0.024 0.046" friction="5 0.1 0.1" contype="1" conaffinity="1"/>
        </body>
      </body>

      <!-- FR LEG -->
      <body name="upperleg_fr" pos="0.72627 -1.2165 0.72616" quat="0.388294 1.9587e-06 0.921535 1.9587e-06">
        <inertial pos="0.024758 -0.02325 0.024007" quat="0.275621 0.27562 -0.651178 0.651178" mass="0.060463" diaginertia="1.94562e-05 1.45858e-05 1.3106e-05"/>
        <joint name="hip_fr" pos="0 0 0" axis="0 1 0" range="-1.57 1.57"/>
        <geom material="gray" mesh="upperleg_fr" class="visual"/>
        <body name="lowerleg_fr" pos="0.039486 0 0.038287" quat="-3.37493e-06 0.394731 1.44993e-06 0.918797">
          <inertial pos="-4.43034e-09 0.02325 0.0364632" quat="1 -8.27384e-07 3.70883e-12 -4.4826e-06" mass="0.0699581" diaginertia="3.15837e-05 2.19759e-05 1.91147e-05"/>
          <joint name="knee_fr" pos="0 0 0" axis="0 1 0" range="-1.57 1.57"/>
          <geom material="gray" mesh="lowerleg_fr" class="visual" friction="5 0.1 0.1"/>
          <!-- Collision sphere -->
          <geom name="foot_fr" type="sphere" size="0.02" pos="0 0.024 0.046" friction="5 0.1 0.1" contype="1" conaffinity="1"/>
        </body>
      </body>

      <!-- BL LEG -->
      <body name="upperleg_bl" pos="0.55677 -1.3166 0.72616" quat="-3.64141e-06 0.991343 4.82291e-07 0.1313">
        <inertial pos="-0.00976295 0.0232502 0.0342534" quat="0.700479 0.700472 -0.0966121 0.0966111" mass="0.0573353" diaginertia="1.79861e-05 1.30651e-05 1.20634e-05"/>
        <joint name="hip_bl" pos="0 0 0" axis="0 1 0" range="-1.57 1.57"/>
        <geom material="gray" mesh="upperleg_bl" class="visual"/>
        <body name="lowerleg_bl" pos="-0.015108 0 0.052884" quat="0.990733 0 -0.135824 0">
          <inertial pos="-4.50047e-09 0.02325 0.0364632" quat="1 -8.09409e-07 3.62809e-12 -4.48239e-06" mass="0.0699581" diaginertia="3.15837e-05 2.19759e-05 1.91147e-05"/>
          <joint name="knee_bl" pos="0 0 0" axis="0 1 0" range="-1.57 1.57"/>
          <geom material="gray" mesh="lowerleg_bl" class="visual" friction="5 0.1 0.1"/>
          <!-- Collision sphere -->
          <geom name="foot_bl" type="sphere" size="0.02" pos="0 0.024 0.046" friction="5 0.1 0.1" contype="1" conaffinity="1"/>
        </body>
      </body>

      <!-- BR LEG -->
      <body name="upperleg_br" pos="0.55677 -1.2016 0.72616" quat="-3.63807e-06 -0.990435 5.06823e-07 -0.137978">
        <inertial pos="-0.00976295 0.0232502 0.0342534" quat="0.700479 0.700472 -0.0966121 0.0966111" mass="0.0573353" diaginertia="1.79861e-05 1.30651e-05 1.20634e-05"/>
        <joint name="hip_br" pos="0 0 0" axis="0 1 0" range="-1.57 1.57"/>
        <geom material="gray" mesh="upperleg_br" class="visual"/>
        <body name="lowerleg_br" pos="-0.015108 0 0.052884" quat="0.988852 0 -0.148904 0">
          <inertial pos="-4.47947e-09 0.02325 0.0364632" quat="1 -8.14809e-07 3.65234e-12 -4.48245e-06" mass="0.0699581" diaginertia="3.15837e-05 2.19759e-05 1.91147e-05"/>
          <joint name="knee_br" pos="0 0 0" axis="0 1 0" range="-1.57 1.57"/>
          <geom material="gray" mesh="lowerleg_br" class="visual" friction="5 0.1 0.1"/>
          <!-- Collision sphere -->
          <geom name="foot_br" type="sphere" size="0.02" pos="0 0.024 0.046" friction="5 0.1 0.1" contype="1" conaffinity="1"/>
        </body>
      </body>

    </body>
  </worldbody>

  <!-- MOTORS -->
  <actuator>
    <motor name="hip_fl_motor" joint="hip_fl" ctrlrange="-1.0 1.0" gear="100"/>
    <motor name="knee_fl_motor" joint="knee_fl" ctrlrange="-1.0 1.0" gear="100"/>
    <motor name="hip_fr_motor" joint="hip_fr" ctrlrange="-1.0 1.0" gear="100"/>
    <motor name="knee_fr_motor" joint="knee_fr" ctrlrange="-1.0 1.0" gear="100"/>
    <motor name="hip_bl_motor" joint="hip_bl" ctrlrange="-1.0 1.0" gear="100"/>
    <motor name="knee_bl_motor" joint="knee_bl" ctrlrange="-1.0 1.0" gear="100"/>
    <motor name="hip_br_motor" joint="hip_br" ctrlrange="-1.0 1.0" gear="100"/>
    <motor name="knee_br_motor" joint="knee_br" ctrlrange="-1.0 1.0" gear="100"/>
  </actuator>

  <keyframe>
    <key name="home" qpos="
    0 0 -0.5
    1 0 0 0
    -0.75 -0.75 -0.75 -0.75
    -0.75 0.75 -0.75 0.75"
      ctrl="
    -0.75 -0.75 -0.75 -0.75
    -0.75 0.75 -0.75 0.75"/>
  </keyframe>

</mujoco>
